{
  "manifest_version": 2,
  "author": "Mathieu Kim Robin",
  "name": "Never-Consent",
  "description": "Never consent to any GDPR consent management platform",
  "version": "1.9.2",
  "icons": {
    "16": "assets/16.ico",
    "48": "assets/48.ico",
    "128": "assets/128.ico",
    "100": "assets/never-consent-small.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "assets/128.ico",
    "default_title": "Never-Consent"
  },
  "permissions": [],
  "web_accessible_resources": [
    "assets/*",
    "src/nc.js"
  ]
}
